 <% if @data.present? %>
        <%= form_with model: @datum  do |f| %>
            <div class="flex flex-col items-center my-4">
            <p class="">Remove rows:</p>
            <% data_array = @data.all.map { |n| @data } %>
            <%= f.select(:header, options_for_select(data_array)) %>
            <button><%= f.submit "Remove", method: :delete, class: "border-red-600 rounded bg-red-600 button hover:bg-red-700 px-3 py-2 my-2 text-white" %></button>
            </div>
        <% end %>
    <% end %>